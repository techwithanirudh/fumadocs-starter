  },
  "peerDependencies": {
    "ajv": "^8.17.1"
  },
  "engines": {
    "node": ">=22"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "esbuild",
    "lefthook",
    "msw",
    "sharp",
    "unrs-resolver"
  ],
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
