{
  "name": "fumadocs-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run build:pre && next build && bun run build:post",
    "build:pre": "bun ./scripts/pre-build.ts",
    "build:post": "bun ./scripts/post-build.ts",
    "dev": "next dev",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "check": "biome check .",
    "check:spelling": "cspell -c .cspell.jsonc --no-progress --no-summary --no-must-find-files --unique --cache",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "lint": "fumadocs-mdx && bun ./scripts/lint.ts && biome lint",
    "lint:write": "biome lint --write .",
    "lint:unsafe": "biome lint --write --unsafe .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit --incremental false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.59",
    "@ai-sdk/react": "^2.0.81",
    "@bprogress/next": "^3.2.12",
    "@fumadocs/mdx-remote": "^1.4.3",
    "@next/bundle-analyzer": "16.0.1",
    "@next/env": "16.0.1",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@shikijs/rehype": "^3.14.0",
    "@shikijs/transformers": "^3.14.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@takumi-rs/core": "^0.40.1",
    "@takumi-rs/helpers": "^0.40.1",
    "@takumi-rs/image-response": "^0.40.1",
    "ai": "^5.0.85",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "^16.0.5",
    "fumadocs-docgen": "^3.0.3",
    "fumadocs-mdx": "^13.0.2",
    "fumadocs-openapi": "^9.7.1",
    "fumadocs-twoslash": "^3.1.9",
    "fumadocs-typescript": "^4.0.12",
    "fumadocs-ui": "^16.0.5",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "jiti": "^1.21.7",
    "katex": "^0.16.25",
    "lucide-react": "^0.552.0",
    "mermaid": "^11.12.1",
    "motion": "^12.23.24",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "oxc-transform": "^0.56.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rehype-katex": "^7.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.1",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "rimraf": "^6.0.1",
    "shiki": "^3.14.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "twoslash": "^0.3.4",
    "unist-util-visit": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@cspell/dict-bash": "^4.2.2",
    "@fumadocs/cli": "^1.0.3",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/bun": "^1.3.1",
    "@types/hast": "^3.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "chokidar": "^4.0.3",
    "cspell": "^9.2.2",
    "dotenv-cli": "^10.0.0",
    "gray-matter": "^4.0.3",
    "lefthook": "^2.0.1",
    "lint-staged": "^16.2.6",
    "next-validate-link": "^1.6.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "shadcn": "^3.5.0",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^4.1.16",
    "tinyglobby": "^0.2.15",
    "ts-morph": "^27.0.2",
